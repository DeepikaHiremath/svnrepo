<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Page
    canEdit="true"
    countsAsWork="false"
    id="SerializationInfo"
    parent="InfoPages()"
    startInEditMode="true"
    title="displaykey.Web.InternalTools.InfoPages.SerializationInfo.Title">
    <Variable
      initialValue="true"
      name="includingWhitelistedClasses"
      type="boolean"/>
    <Screen>
      <Verbatim
        label="displaykey.Web.InternalTools.InfoPages.SerializationInfo.DeserializedClasses"/>
      <DetailViewPanel>
        <InputColumn>
          <CheckBoxInput
            editable="true"
            id="IncludingWhitelistedClasses"
            labelAbove="true"
            value="includingWhitelistedClasses"
            valueLabel="displaykey.Web.InternalTools.InfoPages.SerializationInfo.IncludingWhitelistedClasses">
            <PostOnChange/>
          </CheckBoxInput>
          <PreFormattedTextInput
            id="DeserializedClasses"
            labelAbove="true"
            value="gw.pl.util.HTMLEscapeUtil.escape(gw.api.tools.SerializationInfoPageHelper.getDeserializedClassNames(includingWhitelistedClasses))"/>
        </InputColumn>
      </DetailViewPanel>
    </Screen>
  </Page>
</PCF>